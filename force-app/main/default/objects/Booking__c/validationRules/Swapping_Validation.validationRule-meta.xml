<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Swapping_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>AND(!Is_Swapped__c,ISPICKVAL(Stage__c, &apos;Swapping&apos;) )</errorConditionFormula>
    <errorMessage>Stage cannot be changed to swapping, Please request for swapping</errorMessage>
</ValidationRule>
