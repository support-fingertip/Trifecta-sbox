<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Age_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>Age__c &lt; 18</errorConditionFormula>
    <errorDisplayField>Age__c</errorDisplayField>
    <errorMessage>Age must be 18 or older</errorMessage>
</ValidationRule>
