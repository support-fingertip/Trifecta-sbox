<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Block_PreSales_Edit_When_Assigned_To_Sal</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
    ISCHANGED(LastModifiedDate),
    $Profile.Name = &quot;Pre Sales&quot;,
    SLead__r.Owner:User.Profile.Name = &quot;Sales&quot;
)</errorConditionFormula>
    <errorMessage>You cannot edit Follow Ups because this Lead is assigned to the Sales Team.</errorMessage>
</ValidationRule>
